{
    "compilerOptions": {
        "incremental": true,
        "target": "es5",
        "module": "commonjs",
        "downlevelIteration": true,
        "strict": true,
        "noImplicitAny": false,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,

        "declaration": true,
        "outDir": "./dist"
    },
    "include": ["src/**/*.ts"]
}
